package minedeath;

import org.apache.commons.codec.language.bm.Lang;
import org.bukkit.ChatColor;
import org.bukkit.command.ConsoleCommandSender;
import org.bukkit.plugin.java.JavaPlugin;

import minedeath.world.WorldMain;


public class Main extends JavaPlugin{
	Lang l = lang.;
	public void onEnable(){
		getServer().getPluginManager().registerEvents(new WorldMain(), this);
		l.print(ChatColor.GREEN + "MineDeath Has Been Enabled");
		
	}
	public static ConsoleCommandSender s = getServer().getConsoleSender();
	
}
