package minedeath.world;

import java.util.ArrayList;

import org.bukkit.block.Block;
import org.bukkit.block.Sign;
import org.bukkit.event.Listener;

import lang.Lang;

public class WorldMain implements Listener{
	Block block;
	Sign sign;
	ArrayList<String> signText;
	private static void print(String arg){
		Lang.print(arg);
	}
	public void readSign(Sign sign){
		for (String text : sign.getLines()){
			if(!text.matches("")){
				signText.add(text);	
			}
			
		}
		print(parseSign(signText));
		return;
	}
	private String parseSign(ArrayList<String> list){
		return list.toString();
	}
}
