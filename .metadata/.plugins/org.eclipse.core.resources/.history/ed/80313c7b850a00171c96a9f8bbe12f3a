package games;

import java.util.HashMap;

import flags.Bool;
import flags.Flag;

public class GamesMain {
	private HashMap<String, Flag<?>> flags = new HashMap<String, Flag<?>>();
	private Bool teams = new Bool("teams", false);
	
	public void setupFlags(){
		addflag(teams.getName(), teams);
	}
	private void addFlag(String name, Flag<?> flag){
		flags.put(name, flag);
	}
}
