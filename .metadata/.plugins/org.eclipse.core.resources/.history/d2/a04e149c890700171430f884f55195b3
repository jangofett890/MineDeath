package lang;
import org.bukkit.Bukkit;
import org.bukkit.command.CommandSender;

import net.md_5.bungee.api.ChatColor;

public class Lang {
	private enum Status{INFO, SEVERE, COMPLETE};
	private Lang(){}
	private static Lang instance = new Lang();
	public static Lang getInstance(){
		return instance;
	}
	private void sendMessage(CommandSender src, String msg, Status status){
		src.sendMessage(color + msg);
	}
	public static void print(String arg, Status status){
		Bukkit.getServer().getConsoleSender().sendMessage(arg);
	}
}
